                    },
                    "400": {
                        "description": "Bad Request"
                    },
                    "404": {
                        "description": "not found"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/users": {
            "put": {
                "tags": [
                    "User"
                ],
                "summary": "Update user",
                "operationId": "fe5199ce950b39e4f6437d0eac7aafd3",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
